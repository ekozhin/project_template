pipeline {
    agent any
    options {
        timestamps()
    }
    stages {
        stage ('Init'){
            steps {
                sh 'ls -la'
            }
        }
        stage ('Build') {
            steps {
                echo "Build step..."
            }
        }
        stage ('Deploy') {
            steps {
                echo "Deploy step..."
            }
        }
    }
}
